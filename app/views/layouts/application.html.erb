<!DOCTYPE html>
<html>
<head>
  <title>Network</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div style="float:right">
	<%if !session[:user].nil?%>
		Hi <%=session[:user].email%></br>
		<a href="/users" >Home</a><br>
		<a href="/contacts" >Contacts</a><br>
		<div style="cursor:pointer;" onclick="userdeletehtml('<%=session[:user].id%>')">Delete my account</div>

		<br><a href="/logout" >logout</a>

		
	<%else%>
		<a href="/login_new" >Login</a>
	<%end%> 
</div>
<%= yield %>

</body>
</html>
